{
  "name": "pos-printer-service",
  "version": "1.0.0",
  "description": "A local printer service for POS systems that handles thermal receipt printing via CUPS",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "npm run test:unit",
    "test:unit": "jest --testPathPattern=tests/unit",
    "test:integration": "jest --testPathPattern=tests/integration",
    "install-service": "node scripts/install-service.js",
    "uninstall-service": "node scripts/uninstall-service.js",
    "setup": "node scripts/setup.js"
  },
  "keywords": [
    "pos",
    "printer",
    "receipt",
    "thermal",
    "cups",
    "linux"
  ],
  "author": "POS System Developer",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "sharp": "^0.32.6",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "jest": "^29.6.2",
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/pos-printer-service.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/pos-printer-service/issues"
  },
  "homepage": "https://github.com/your-org/pos-printer-service#readme"
}
